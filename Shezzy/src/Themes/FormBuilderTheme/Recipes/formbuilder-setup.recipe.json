{
  "name": "form-builder-theme-setup",
  "displayName": "Form Builder Theme",
  "description": "Form Builder Theme.",
  "author": "Form Builder Theme",
  "website": "https://us.ro",
  "version": "1.0.0",
  "issetuprecipe": true,
  "categories": [ "formbuilder" ],
  "tags": [ "innovation", "inno" ],
  "steps": [
    {
      "name": "Feature",
      "enable": [
        "OrchardCore.Admin",
        "OrchardCore.Alias",
        "OrchardCore.Autoroute",
        "OrchardCore.ContentFields",
        "OrchardCore.ContentPreview",
        "OrchardCore.Contents",
        "OrchardCore.ContentTypes",
        "OrchardCore.Deployment",
        "OrchardCore.Diagnostics",
        "OrchardCore.DynamicCache",
        "OrchardCore.Email",
        "OrchardCore.ReCaptcha",
        "OrchardCore.Features",
        "OrchardCore.Flows",
        "OrchardCore.Forms",
        "OrchardCore.HomeRoute",
        "OrchardCore.Html",
        "OrchardCore.Localization",
        "OrchardCore.Media",
        "OrchardCore.Navigation",
        "OrchardCore.Menu",
        "OrchardCore.Recipes",
        "OrchardCore.Resources",
        "OrchardCore.Roles",
        "OrchardCore.Scripting",
        "OrchardCore.Security",
        "OrchardCore.Settings",
        "OrchardCore.Shortcodes.Templates",
        "OrchardCore.Templates",
        "OrchardCore.Tenants",
        "OrchardCore.Themes",
        "OrchardCore.Title",
        "OrchardCore.Users",
        "OrchardCore.Users.Registration",
        "OrchardCore.Widgets",
        "OrchardCore.Workflows",
        "OrchardCore.Workflows.Http",
        "FormBuilderTheme"
      ]
    },
    {
      "name": "themes",
      "admin": "TheAdmin",
      "site": "FormBuilderTheme"
    },
    {
      "name": "Settings",
      "Calendar": null,
      "UseCdn": false,
      "TimeZoneId": "America/Toronto",
      "AppendVersion": true,
      "LocalizationSettings": {
        "DefaultCulture": "en",
        "SupportedCultures": [ "en" ]
      },
      "AdminSettings": {
        "DisplayMenuFilter": true
      },
      "LoginSettings": {
        "UseSiteTheme": true
      },
      "ResetPasswordSettings": {
        "UseSiteTheme": true
      },
      "RegistrationSettings": {
        "UseSiteTheme": true
      },
      "LayerSettings": {
        "Zones": [
          "Header",
          "Breadcrumb",
          "NavigationDrawer",
          "Footer",
          "IEBanner"
        ]
      }
    },
    {
      "name": "Layers",
      "Layers": [
        {
          "Name": "All",
          "Rule": "true",
          "Description": null
        }
      ]
    }
  ]
}
